#!/bin/sh

set -o errexit
set -o pipefail
set -o nounset
pwd
ls -lah

ls -lah /app
ls -lah /app/api

python manage.py collectstatic --noinput
python manage.py migrate

echo "Starting gunicorn!"
gunicorn config.wsgi:application --bind 0.0.0.0:$PORT --workers 1

#daphne config.asgi:application -b 0.0.0.0 -p 8000
